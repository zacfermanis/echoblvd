{
  "headers": [
    {
      "source": "/(.*)\\.svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)\\.svg",
      "destination": "/$1.svg"
    }
  ]
} 